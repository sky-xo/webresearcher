{
  "name": "webresearcher",
  "version": "1.0.0",
  "description": "CLI tool that uses GPT-5.2 with web search for up-to-date research queries",
  "type": "module",
  "bin": {
    "webresearcher": "./dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outfile dist/index.js --target node && sed -i '' '1s|#!/usr/bin/env bun|#!/usr/bin/env node|' dist/index.js",
    "prepublishOnly": "bun run build",
    "dev": "bun run src/index.ts"
  },
  "files": [
    "dist",
    ".claude"
  ],
  "keywords": [
    "ai",
    "web-search",
    "research",
    "gpt",
    "openai",
    "cli",
    "claude-code",
    "agent"
  ],
  "author": "sky-xo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sky-xo/webresearcher.git"
  },
  "bugs": {
    "url": "https://github.com/sky-xo/webresearcher/issues"
  },
  "homepage": "https://github.com/sky-xo/webresearcher#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "openai": "^5"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
